<div class="vertical light">
    <div class="wrapper">
        {{> header}}
        {{> aside-left}}
        <main role="main" class="main-content">
            <div class="container-fluid">
                <div class="row justify-content-center">
                    <div class="col-12">
                        <div class="row my-4">
                            <!-- Small table -->
                            <div class="col-md-12">
                                <div class="card shadow">
                                    <div class="card-body">
                                        <!-- table -->
                                        <div class="table_header">
                                            <span class="d-inline-block" tabindex="0" data-toggle="tooltip"
                                                title="Xem thùng rác">
                                                <a style="text-decoration: none;" href="/markPage/trash">
                                                    <i class="fe fe-trash-2 icon__table-large icon__table-trash"></i>
                                                </a>
                                            </span>
                                            <div class="box__input-headerTable" style="width: 100%;">
                                                <input type="text" class="form-control input-headerTable w-100"
                                                    placeholder="Tìm kiếm ..">
                                                <span class="fe fe-14 fe-search iconSearch_headerTable"></span>
                                            </div>
                                        </div>
                                        <table class="table datatables table-bordered table-hover table-striped">
                                            <thead style="background-color: #F0F0F0;">
                                                <tr>
                                                    <th></th>
                                                    <th style="min-width: 300px;">
                                                        Name</th>
                                                    <th style="max-width: 60px;">Đối tượng</th>
                                                    <th>Trạng thái</th>
                                                    <th>Ngày chấm</th>
                                                    <th>Năm đánh giá</th>
                                                    <th>Actions</th>
                                                </tr>

                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td>
                                                        <div class="approved">Lưu nháp</div>
                                                    </td>

                                                    <td>
                                                        <ul style="list-style-type: none;font-weight: 600;padding: 0;">
                                                            <!-- name product -->
                                                            <li data-toggle="modal" data-target="#exampleModal"></li>
                                                            <p class="nameProduct" data-toggle="modal"
                                                                data-target="#exampleModal2">101: Thành Phố Từ Sơn ,
                                                                tỉnh Bắc Ninh
                                                            </p>
                                                            </li>
                                                        </ul>
                                                    </td>
                                                    <!-- product group -->
                                                    <td style="max-width: 150px;">Xã nông thôn mới</td>
                                                    <!-- nam thanh lap -->
                                                    <td style="font-weight: 600;color: green;">Đạt</td>
                                                    <td>16/10/2003</td>
                                                    <td style="font-weight: 600;">2024</td>
                                                    <td>
                                                        <span class="d-inline-block" tabindex="0" data-toggle="tooltip"
                                                            title="Chỉnh sửa">
                                                            <a style="text-decoration: none;" href="/markPage/update/1">
                                                                <i class="fe fe-edit-3 icon__table icon__table-edit">
                                                                </i>
                                                            </a>
                                                        </span>
                                                        <span class="d-inline-block" tabindex="0" data-toggle="tooltip"
                                                            title="Xác nhận">
                                                            <i
                                                                class="fe fe-check-circle icon__table icon__table-success"></i>
                                                        </span>
                                                        <span class="d-inline-block" tabindex="0" data-toggle="tooltip"
                                                            title="Xem chi tiết">
                                                            <i class="fe fe-eye icon__table icon__table-eye"
                                                                data-toggle="modal" data-target="#exampleModal2"></i>
                                                        </span>
                                                        <span class="d-inline-block" tabindex="0" data-toggle="tooltip"
                                                            title="Chuyển vào thùng rác">
                                                            <i class="fe fe-trash-2 icon__table icon__table-trash"></i>
                                                        </span>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                        <!-- table -->
                                        {{> modalMarkPage}}
                                        <!-- table -->
                                    </div>
                                </div>
                            </div> <!-- simple table -->
                        </div> <!-- end section -->
                    </div> <!-- .col-12 -->
                </div> <!-- .row -->
            </div> <!-- .container-fluid -->
        </main> <!-- main -->
    </div>
</div>